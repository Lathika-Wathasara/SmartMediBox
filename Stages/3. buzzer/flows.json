    {
        "id": "d1e7936ecf9ad9f9",
        "type": "function",
        "z": "94b8089ddde97015",
        "name": "scheduling_func",
        "func": "let schedule_state = flow.get('schedule_state');\nlet day_1 = flow.get('repeat_days');\nlet day_2 = flow.get('repeat_days');\nlet day_3 = flow.get('repeat_days');\nlet time_1 = flow.get('time_1');\nlet time_2 = flow.get('time_2');\nlet time_3 = flow.get('time_3');\n\n\nlet hour = parseInt(msg.payload.substring(0,2));\nlet mn = parseInt(msg.payload.substring(3, 5));\n\nvar Msg;\n\nif (schedule_state) {\n    if (day_1 > 0) {\n        let t_hour = time_1.hr;\n        let t_mn = time_1.mn;\n        if ((t_hour > hour) || ((t_mn > mn) && (t_hour == hour))) {\n            day_1 = day_1 - 1;\n            Msg = { payload: \"0\" };\n        }\n        else if ((t_hour == hour) && (t_mn == mn)) {\n            day_1 = day_1 - 1;\n            Msg = { payload: \"1\" };\n            return Msg;\n        }\n        else {\n            Msg = { payload: \"0\" };\n        }\n    }\n    if (day_2 > 0) {\n        let t_hour = time_2.hr;\n        let t_mn = time_2.mn;\n        if ((t_hour > hour) || ((t_mn > mn) && (t_hour == hour))) {\n            day_2 = day_2 - 1;\n            Msg = { payload: \"0\" };\n        }\n        else if ((t_hour == hour) && (t_mn == mn)) {\n            day_2 = day_2 - 1;\n            Msg = { payload: \"1\" };\n            return Msg;\n        }\n        else {\n            Msg = { payload: \"0\" };\n        }\n    }\n    if (day_3 > 0) {\n        let t_hour = time_3.hr;\n        let t_mn = time_3.mn;\n        if ((t_hour > hour) || ((t_mn > mn) && (t_hour == hour))) {\n            day_2 = day_3 - 1;\n            Msg = { payload: \"0\" };\n        }\n        else if ((t_hour == hour) && (t_mn == mn)) {\n            day_3 = day_3 - 1;\n            Msg = { payload: \"1\" };\n            return Msg;\n        }\n        else {\n            Msg = { payload: \"0\" };\n        }\n    }\n    }\n\nreturn Msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
